{
  "name": "aidchat",
  "integrations": {
    "capacitor": {}
  },
  "type": "custom",
  "plugins": {
    "CapacitorHttp": {
      "enabled": true
    },
    "Keyboard": {
      "resize": "body",
      "style": "default",
      "resizeOnFullScreen": true
    },
    "PushNotifications": {
      "presentationOptions": ["badge", "sound", "alert"]
    },
    "Device": {
      "orientation": "portrait-primary",
      "hideStatusBar": true
    }
  }
}
